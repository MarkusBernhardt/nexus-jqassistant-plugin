<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Nexus">
	<description>Provides descriptors for Nexus.</description>
	<model>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.MavenReleaseDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.MavenSnapshotDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.NexusDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.NexusMavenArtifactDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.NexusMavenRepositoryDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.NexusRepositoryDescriptor</class>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.descriptors.NexusStorageItemDescriptor</class>
	</model>
	<scanner>
		<class>com.github.markusbernhardt.nexus.plugins.jqassistant.backend.scanner.NexusStorageItemScannerPlugin</class>
	</scanner>
</jqa-plugin:jqassistant-plugin>